{
    "name": "dmk/nc_usefulpages",
    "description": "Allows users to rate the current visited page as useful or not useful and give feedback.",
    "type": "typo3-cms-extension",
    "homepage": "http://www.dmk-ebusiness.de/",
    "license": "GPL-2.0+",
    "require": {
      "typo3/cms-core": "~8.7 || ~9.5"
    },
	"replace" : {
		"nc_usefulpages" : "self.version",
		"typo3-ter/nc_usefulpages" : "self.version"
	},	
	"autoload" : {
		"psr-4" : {
			"Netcreators\\NcUsefulpages\\" : "Classes/"
		}
	}	
}
